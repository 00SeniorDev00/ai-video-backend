{
    "version": 2,
    "builds": [
        {
        "src": "./index.js", 
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/index.js"
        }
    ],
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    },
    "installCommand": "pnpm install",
    "buildCommand": "pnpm run build",
    "outputDirectory": "dist"
}